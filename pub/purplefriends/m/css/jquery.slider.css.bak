.jquery-slider {
    overflow: hidden;
    position: relative;
}
.jquery-slider-element {
    overflow: hidden;
    display: none;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}
.jquery-slider-control {
    overflow: hidden;
    position: absolute;
    text-align: center;
    width: 24px;
    height: 24px;
    line-height: 24px;
    font-size: 16px;
    font-weight: bold;
    padding: 0;
    margin: 0;
    _border: 1px solid #666;
    _background: #fff;
    _opacity: 0.33;
    cursor: pointer;


}
.jquery-slider-control:hover {
	
    opacity: 1;
}
.jquery-slider-control-prev {
	display:block;
	background:url('slider_prev.png') no-repeat;
	width:34px;
	height:56px;
	text-indent:-9999px;
    left: 35px;
    top: 120px;
}
.jquery-slider-control-next {
	background:url(slider_next.png') no-repeat;
	width:34px;
	height:56px;
	text-indent:-9999px;
    right: 30px;
    top: 120px;
}
.jquery-slider-pages {
    overflow: visible;
    position: absolute;
    left: 10px;
    top: 10px;
    height: 20px;
    right: 5px;
}
.jquery-slider-page {
    overflow: hidden;
    position: relative;
    _display: block;
    float: right;
	cursor: pointer;

	display:none;
	width:10px;
	height:10px;
	margin:0 5px 0 0;
	background:url(images/fs.pager1.png) 0px 0px no-repeat transparent;
}
.jquery-slider-page:hover {
	
}
.jquery-slider-page-current {
	width:36px;
	height:10px;
	background:url("../img/pager_over.png") no-repeat;
    opacity: 1;
}
